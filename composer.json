        "status"
    ],
    "homepage": "https://jdz.joffreydemetz.com/http/",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Joffrey Demetz",
            "email": "joffrey.demetz@gmail.com",
